        <!--&lt;!&ndash;去除ActionBar&ndash;&gt;-->
        <!--<item name="windowActionBar">false</item>-->
        <!--<item name="windowNoTitle">true</item>-->
    </style>
    <style name="item_block_style">
        <item name="android:background">#ffffff</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">72dp</item>
        <item name="android:paddingLeft">22dp</item>
        <item name="android:paddingRight">22dp</item>
    </style>
    <style name="line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_marginLeft">22dp</item>
        <item name="android:layout_marginRight">22dp</item>
        <item name="android:background">#e8e8e8</item>
    </style>
    <style name="normal_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#444444</item>
    </style>
    <style name="selected_img_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:src">@drawable/selected</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="switch_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:track">@drawable/switch_track</item>
        <item name="android:thumb">@drawable/switch_thumb</item>
        <item name="android:textOn">""</item>
        <item name="android:textOff">""</item>
        <item name="android:switchMinWidth">0dip</item>
        <!--<item name="switchTextAppearance">@style/Widget.AppCompat.Button.Borderless</item>-->
        <!--<item name="borderlessButtonStyle">@style/Widget.AppCompat.Button.Borderless</item>-->
    </style>
